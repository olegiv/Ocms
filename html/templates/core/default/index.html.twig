<!DOCTYPE html>
<html>
	<head>
		{% block head %}
			{{- include ('include/head.html.twig') }}
		{% endblock %}
	</head>
	<body{% if bodyClasses %} class="{{ bodyClasses }}"{% endif %}>
		{% block header %}
			{{- include ('include/header/header.html.twig') }}
		{% endblock %}
		{% block body %}
			<div id="content">
				{% block content %}
					<h1 class="node-title">{{ title }}</h1>
					{{ body | raw }}
				{% endblock %}
			</div>
		{% endblock %}
		<div id="footer">
			{% block footer %}
				{{- include ('include/footer/footer.html.twig') }}
			{% endblock %}
		</div>
	</body>
</html>
